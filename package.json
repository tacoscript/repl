{
  "name": "esprima_harmony_demo",
  "version": "0.1.0",
  "description": "Online demo of FB's esprima harmony branch",
  "repository": {
    "type": "git",
    "url": "https://github.com/fkling/esprima_ast_explorer.git"
  },
  "author": "Felix Kling",
  "license": "MIT",
  "devDependencies": {
    "babelify": "^6.1.3",
    "brfs": "^1.1.1",
    "browserify": "^11.0.0",
    "envify": "^3.2.0",
    "http-server": "^0.8.0",
    "partition-bundle": "^2.0.0",
    "reactify": "^1.0.0",
    "uglify-js": "^2.4.16",
    "uglifyify": "^3.0.1",
    "watchify": "^3.2.3"
  },
  "dependencies": {
    "babel-core": "^5.7.3",
    "babel-runtime": "^5.8.19",
    "classnames": "^2.1.3",
    "codemirror": "^5.1.0",
    "escodegen": "^1.4.1",
    "esprima-fb": "^15001.1.0-dev-harmony-fb",
    "jscodeshift": "^0.3",
    "keypress": "git://github.com/dmauro/Keypress",
    "pubsub-js": "^1.4.2",
    "react": "^0.13.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "stage": 0,
          "optional": [
            "runtime"
          ]
        }
      ],
      "envify",
      "brfs"
    ]
  },
  "scripts": {
    "start": "http-server",
    "build": "NODE_ENV=production browserify -p [partition-bundle --map partition.json --output ./ --main ./src/app] -g uglifyify",
    "dev": "NODE_ENV=devlepment browserify -p [partition-bundle --map partition.json --output ./ --main ./src/app] -g uglifyify"
  }
}
